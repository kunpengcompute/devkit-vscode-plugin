{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "tuning-ide": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "tuning",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../extension/tuning/out/sysperfanalysis",
            "index": "tuning/index.html",
            "main": "tuning/main.ts",
            "polyfills": "tuning/polyfills.ts",
            "tsConfig": "tuning/tsconfig.app.json",
            "aot": true,
            "assets": [
              "tuning/favicon.ico",
              "tuning/assets",
              {
                "glob": "**",
                "input": "common/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "tuning/styles.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "tuning/environments/environment.ts",
                  "with": "tuning/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "14mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tuning-ide:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tuning-ide:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tuning-ide:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
