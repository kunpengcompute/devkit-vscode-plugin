{
  "name": "tuning-Kunpeng_DevKit_Frontend-plugin",
  "version": "2.3.5",
  "main": "index.js",
  "scripts": {
    "install": "cd workspace && npm install --unsafe-perm && cd ../extension/tuning && npm install",
    "package:vscode": "cd extension/tuning && npm run package",
    "package:intellij": "cd extension/tuning-intellIJ && sh build/build-webview.sh",

    "package0": "cd extension/tuning && npm run package0"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "workspace/**/*.ts": "tslint -c ./workspace/tslint.json",
    "extension/**/*.ts": "tslint -c ./extension/tuning/tslint.json"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.10"
  }
}
